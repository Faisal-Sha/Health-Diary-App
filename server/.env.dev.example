# Copy this file to .env.dev for local development with docker-compose.dev.yml
# These values match the defaults defined in docker-compose.dev.yml.

# Flask configuration
FLASK_ENV=development
FLASK_CONFIG=config.DevelopmentConfig
FLASK_DEBUG=True
FLASK_APP=app.py
FLASK_PORT=5000

# Security / integrations
JWT_SECRET_KEY=dev-secret-change-me
OPENAI_API_KEY=

# Database connection string (points to the "db" service defined in docker-compose.dev.yml)
DATABASE_URL=postgresql://username:password@db:5432/health_app
